  },
  "reactSnap": {
    "source": "dist",
    "publicPath": "/",
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
    "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
