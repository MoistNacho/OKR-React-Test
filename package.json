    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^7.0.1",
    "jdk": "^0.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^11.1.1",
    "prettier": "^2.1.2",
    "prettier-eslint": "^9.0.1",
    "react-test-renderer": "^17.0.2",
    "redoc-cli": "^0.9.2",
