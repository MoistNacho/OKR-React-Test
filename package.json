{
  "name": "okr-react-test",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Junsung",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "cross-env stage=$stage webpack-dev-server --config webpack.dev.js",
    "lint": "eslint src --ext .js,.ts,.tsx --max-warnings 0 --cache",
    "check-type": "tsc --noEmit --skipLibCheck",
    "test": "jest --passWithNoTests",
    "test:all": "concurrently \"yarn test\" \"yarn cypress:run\"",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --collectCoverage --passWithNoTests",
    "prebuild": "yarn build:clean",
    "build": "cross-env stage=$stage webpack --config webpack.prod.js",
    "build:actions": "cross-env stage=dev1 webpack --config webpack.actions.js",
    "build:clean": "rimraf ./dist",
    "build:profile": "yarn build --profile",
    "generate-apis": "node ./scripts/generateApis.js",
    "redoc": "redoc-cli serve -p 30001",
    "cypress:open": "cross-env stage=$stage cypress open",
    "cypress:run": "cross-env stage=$stage cypress run",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "prepush": "concurrently \"yarn check-type\" \"yarn test\"",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "src/**/*.tsx": [
      "prettier --write",
      "eslint --fix --max-warnings 0",
      "yarn stylelint"
    ]
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
